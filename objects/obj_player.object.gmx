        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Repel cars with shield if enabled
if ( repel_radius &gt; 0 ) {
    var repel_strength = 10; // how much pixels to repel car
    var car;
    car = collision_circle( x, y, repel_radius, obj_crashable, false, true )
    var i = 0;
    while ( car != noone and i &lt; 100 ) { // Max 100 repels. Otherwise game can stuck
        var repel_x = min(repel_strength);
        var repel_y = min(repel_strength);
        
        if ( car.x &gt;= x ) then car.x += repel_x;
        else car.x -= repel_x;
        
        if ( car.y &gt;= y ) then car.y += repel_x;
        else car.y -= repel_x;
        
        audio_play_sound_on(car.aem, choose(sound_shields_hit1, sound_shields_hit2, sound_shields_hit3), false, 20);
        camera_shake(room_speed / 10, 1);
        
        if car.vspeed &lt; 0 then car.image_angle = point_direction(x, y, car.x, car.y);
        else car.image_angle = point_direction(x, y, car.x, car.y) + 180;
        car.repeled = true;
        car = collision_circle( x, y, repel_radius, obj_crashable, false, true );
        i++;
    }
}
