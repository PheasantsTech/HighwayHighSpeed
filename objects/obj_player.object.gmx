        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Move and set direction
var speed_h = 4;
var speed_v = 7;

if not (global.slowmo_shader != noone) { // If teleport is enabled it means that we are slowed down and changing our speed will broke the cool effect
    if ( move_left) {
        image_angle_adjust_smoothly(id, 15, 5);
        hspeed = -1 * speed_h * move_left;
    } else if ( move_right ) {
        image_angle_adjust_smoothly(id, -15, 5);
        hspeed = speed_h * move_right;
    } else if ( not spinning ){
        image_angle_adjust_smoothly(id, 0, 5);
        hspeed = 0;
    }

    if ( speed_up ) {
        vspeed = -1 * speed_v * speed_up;
    } else if ( speed_down ) {
        vspeed = min(15, background_vspeed[0] * speed_down);
    } else {
        vspeed = 0;
    }
} else { // Go straight forward
    vspeed = 0;
    hspeed = 0;
    image_angle_adjust_smoothly(id, 0, 0.5);
}
</string>
