            <string>/// Select or buy car type
globalvar achievements_vars, car_info;

if not car_owned {
    if car_price &gt; achievements_vars[? 'cash'] {
        audio_play_sound(sound_click_fail, 50, false);
        exit;
    }
    var car_type = obj_garage_car.curr_car;
    var car_data = car_info[? car_type];
