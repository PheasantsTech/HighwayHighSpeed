        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Increase time speed
globalvar skinset, disable_cars_and_petrol, wanted_level;

if _delorian_speed_up {
    
    if  ((_delorian_tick &lt; 3 * room_speed) and (background_vspeed[source_background_index] &lt; 80)) {
        background_vspeed[source_background_index] ++;
        background_vspeed[0] =  background_vspeed[source_background_index];
    }
    _delorian_tick++;
    if (_delorian_tick == 3 * room_speed) {
        background_vspeed[target_background_index] = 80;
        background_visible[source_background_index] = false;
        background_visible[target_background_index] = true;
        wanted_level = 0;
        while instance_number(obj_police) != 0 {
            var inst = instance_find(obj_police, 0);
            effect_create_above(ef_ring, inst.x, inst.y, 1, c_white);
            with inst instance_destroy();
        }
    }
    if  ((_delorian_tick &gt; 3 * room_speed) and (background_vspeed[target_background_index] &gt; 7)) then background_vspeed[target_background_index] --;
    
    if (_delorian_tick &gt; 5 * room_speed) {
        background_vspeed[source_background_index] = 7;
        disable_cars_and_petrol = false;
        _delorian_speed_up = false;
        switch target_background_index {
            case 1:
                music_start(sound_music_bonkers_for_arcades);
                skinset = "CLASSIC";
                break;
            case 0:
                music_start(sound_music_insane_gameplay);
                skinset = "FUTURE";
                break;
        }
    }
    
    var x1 = obj_player.x - (obj_player.sprite_width * 0.3);
    var x2 = obj_player.x + (obj_player.sprite_width * 0.3);
    
    var y1 = obj_player.y + (obj_player.sprite_height / 2);
    
    var col1 = make_colour_rgb(255,235,39);
    var col2 = make_colour_rgb(255,219,0);
    
    var col_2 = false;
    
    var col;
    
    while y1 &lt; room_height and abs(y1 - obj_player.y) &lt; (_delorian_tick * 5) {
        if col_2 then col = col2;
        else col = col1;
        col_2 = not col_2;
        effect_create_below(ef_flare, x1, y1, 0, col);
        effect_create_below(ef_flare, x2, y1, 0, col);
        y1 += 10;
    }
}
</string>
