            <string>/// Process cheatcode
globalvar achievements_vars;

if ds_map_find_value(async_load, "id") != async_cheatcode then exit;
if not ds_map_find_value(async_load, "status") then exit;

async_cheatcode = noone;

var cheatcode = ds_map_find_value(async_load, "result");

var user_cheated_now = true;
switch cheatcode {
    case "give me money":
        show_message("Here you go!");
        var new_money = irandom(60) + 1;
        for (var i = 0; i &lt; new_money; i++){
            with instance_create(view_xview[0] + random(view_wview[0]), view_yview[0] + random(view_hview[0]), obj_moneycar_coin) {
                alarm[0] = irandom(room_speed * 5) + 1;
                depth = -20000;
            }
        }
        audio_play_sound(sound_yaay, 50, false);
        break;
    case "42":
        if achievements_vars[? 'cheat_42'] {
            audio_play_sound(sound_trombone, 50, false);
            show_message("This is an old one. Everyone knowns that...");
        } else {
            audio_play_sound(sound_ovation, 50, false);
            show_message("That's it! The Answer to the Ultimate Question of Life, the Universe, and Everything!#You are so smart...");
            achievements_vars[? 'cheat_42'] = true;
        }
        break;
    case "explosive touch":
        show_message("Everyone now fears your mighty touch");
        global.explosive_touch = true;
        break;
    case "autoshield":
        global.autoshield = true;
        show_message("AI just got slightly smarter");
        break;
    case "error":
        var max_i = irandom(60) + 10;
        for(var i = 0; i &lt; max_i; i++){
            audio_play_sound(sound_click_fail, 50, false);
            show_message("Error cause by another error when processing your input 'error' from firt error. (" + string(max_i - i) + "/" + string(max_i)+")");
        }
        user_cheated_now = false;
        break;
    default:
        audio_play_sound(sound_click_fail, 50, false);
        show_message(choose("This did... nothing.", "I feel... same.", "What just happened?", "Error. Try typing error to fix the error.", "Seem same as before.", "Nope, that wasn't it.", "Better luck next time.", "Close one", "You are getting closer", "Wow, such try.", "Nope", "Nearly"));
        user_cheated_now = false;
        break;
}

if user_cheated_now then achievements_vars[? 'cheat_used'] += 1;
