        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Create own sprite if everyhing is set
if sprite == noone and create_sprite {
    draw_set_halign(fa_left);
    draw_set_valign(fa_top);
    var background_sprite = sprite_index;
    var text_font = font_achivement_description;
    var bsw = sprite_get_width(background_sprite);
    var bsh = sprite_get_height(background_sprite);
    var surf = surface_create(bsw, bsh);
    surface_set_target(surf);
    draw_sprite(background_sprite, 0, 0, 0);
    var iw = sprite_get_width(icon);
    var ih = sprite_get_height(icon);
    var k;
    if iw &gt; ih then k =  110 / iw;
    else k = 110 / ih;
    var upper_line_y = ((9/131)*bsh);
    draw_sprite_ext( icon, 0, ((13/542)*bsw) + (k*sprite_get_xoffset(icon)), upper_line_y + (k*sprite_get_yoffset(icon)), k, k, 0, c_white, 1 );
    var txtx = ((157/542)*bsw);
    var txtx_maxw = ((520/542)*bsw) - txtx;
    draw_set_color(c_yellow);
    draw_set_font(font_achivement_head);
    var subject_h = string_height(subject);
    subject_h = string_height_ext(subject, subject_h, txtx_maxw);
    draw_text_ext(txtx, upper_line_y, subject, subject_h, txtx_maxw);
    draw_set_color(c_white);
    draw_set_font(text_font);
