        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Create new sprite

globalvar car_info;

if sprite == noone  {
    var car_type = obj_garage_car.curr_car;
    var data = car_info[? car_type];

    draw_set_halign(fa_center);
    draw_set_valign(fa_top);
    var background_sprite = sprite_index;

    var surf = surface_create(sprite_get_width(background_sprite), sprite_get_height(background_sprite));
    surface_set_target(surf);
    draw_sprite(background_sprite, 0, 0, 0);
    
    var text_x = sprite_width / 2;
    var text_y = 5;
    
    draw_set_color(make_colour_rgb(192,6,123));
    draw_set_font(font_garage_title);
    var text = data[? 'name'];
    draw_text(text_x, text_y, text);
    text_y += string_height(text) + (sprite_get_height(background_sprite) * 0.02);
    
    draw_set_halign(fa_left);
    text_x = 40;
    
    var max_line_width = sprite_get_width(background_sprite) - (2 * text_x);
    
    draw_set_font(font_garage_text);
    var text_color = make_colour_rgb(138,43,226);
    var text_x_bck = text_x;
    draw_set_color(text_color);
    
    var lines;
    lines[0] = data[? 'desc'];
    //lines[1] = "";
    lines[1] = "Special ability";
    lines[2] = data[? 'spec'];
    
    var max_i = array_length_1d(lines);
    
    for (var i = 0; i &lt; max_i; i++){
        text = lines[i];
        var th = string_height(text);
        if text == "Special ability" {
            draw_set_color(c_black);
            draw_set_halign(fa_center);
            text_x = sprite_width / 2;
        }
        draw_text_ext(text_x, text_y, text, th, max_line_width);
        if text == "" then text = "A";
        text_y += string_height_ext(text, th, max_line_width);
        if text == "Special ability" {
            draw_set_halign(fa_left);
            draw_set_color(text_color);
            text_x = text_x_bck;
        }
    }
    

    sprite = sprite_create_from_surface(surf, 0, 0, sprite_get_width(background_sprite), sprite_get_height(background_sprite), true, true, 0, 0);
    surface_reset_target();
    surface_free(surf);
}
</string>
