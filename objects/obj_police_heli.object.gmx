            <kind>1</kind>
            <string>///Move ahead of player, drop bomb(s), fly away
if stage == "MOVING_2_PLAYER" {
    tx = obj_player.x;
    ty = max(obj_player.y  - (obj_player.sprite_height / 2) - (sprite_height * 1.5), -1/2 * sprite_height);
    mp_linear_step(tx, ty, moving_speed, false);
    
    if ( abs(x - tx) &lt; 5 and distance_to_point(tx, ty) &lt; moving_speed ){
        stage = "WAITING_4_DROP";
        direction = 90;
        tick = 0;
    }
} else if ( stage == "WAITING_4_DROP" ) or ( stage == "WAITING_4_LEAVE" ) {
    tick ++;
    if ( stage == "WAITING_4_DROP" ) and ( tick &gt;= 1 * room_speed ) {
        instance_create(x, y, obj_bomb);
        bombs --;
        if bombs &lt;= 0 then stage = "WAITING_4_LEAVE";
        else stage = "MOVING_2_PLAYER";
    } else if ( stage == "WAITING_4_LEAVE" ) and ( tick &gt;= 1 * room_speed ) {
        stage = "LEAVING";
        direction = random(360);
        speed = moving_speed;
    }
}

image_angle = direction - 90;
</string>
