          <argument>
            <kind>1</kind>
            <string>///Force other non-dead NPC cars to avoid police
var repel_radius = 0.75 * sprite_width;
var repel_strength = 10; // how much pixels to repel car
for (var i = 0; i &lt; 10 and not dead; i++) { // Upper circle
    var car;
    car = collision_circle( x, y - ( sprite_height * 0.6 ), repel_radius, obj_car, false, true );
    if car == noone then break;
    
    if ( car != noone and not car.dead ) {
        var repel_x = min(repel_strength);
        var repel_y = min(repel_strength);
        
        if ( car.x &gt;= x ) then car.x += repel_x;
        else car.x -= repel_x;
        
        if ( car.y &gt;= y ) then car.y += repel_x;
        else car.y -= repel_x;
        
        car.repeled = true;
    }
}

for (var i = 0; i &lt; 10 and not dead; i++) { // Center circle
    var car;
    car = collision_circle( x, y, repel_radius, obj_car, false, true );
    if car == noone then break;
    if ( car != noone and not car.dead ) {
        var repel_x = min(repel_strength);
        var repel_y = min(repel_strength);
        
        if ( car.x &gt;= x ) then car.x += repel_x;
        else car.x -= repel_x;
        
        if ( car.y &gt;= y ) then car.y += repel_x;
        else car.y -= repel_x;
        
        car.repeled = true;
    }
}

for (var i = 0; i &lt; 10 and not dead; i++) { // Back circle
    var car;
    car = collision_circle( x, y + ( sprite_height * 0.5 ), repel_radius, obj_car, false, true );
    if car == noone then break;
    if ( car != noone and not car.dead ) {
        var repel_x = min(repel_strength);
        var repel_y = min(repel_strength);
        
        if ( car.x &gt;= x ) then car.x += repel_x;
        else car.x -= repel_x;
        
        if ( car.y &gt;= y ) then car.y += repel_x;
        else car.y -= repel_x;
        
        car.repeled = true;
    }
}
</string>
