        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Move to the player and push him away
if mission_completed and not dead{ // Mission: Compeled. Now return to the base.
    if command == "RAMMER" then vspeed = 3;
    else vspeed = -3;
    var target_x = instance_nearest(x, y, obj_car_spawner_up).x;
    hspeed = min(3, abs(target_x - x)) * sign(target_x - x);
} else if not dead {
    var target = obj_player;
    if obj_player.fool_police {
        if instance_exists(obj_player.fool_police) then target = obj_player.fool_police;
        else mission_completed = true;
    }
    if target == id then mission_completed = true; // Wow, that was easy. You captured yourself!
    var target_x;
    
    switch(command){ // Every police car will try antoher aproaching tactic
        case "LEFT": 
           target_x = target.x - (sprite_width / 2) - (target.sprite_width / 2); // Approach from the left
           break;
        case "RIGHT":
           target_x = target.x + (sprite_width / 2) + (target.sprite_width / 2); // Approach from the righ
           break;
        default: // In some another occastion approach from behind
            target_x = target.x;
            break;
    }
    
    if (command != "RAMMER" and ( abs(y - target.y) &gt; 5 )) then vspeed = (1.5 * sign(target.y - y)); // Approach to the same y value as player
    if command == "RAMMER" then vspeed = 4 + background_vspeed[0];
    else if ((target != obj_player) and (target.y &gt; y)) then vspeed += min(0, target.vspeed); // Quicky! Catch the fake target!
    
    if (command != "RAMMER" and ( abs(x - target_x) &gt; 5 )) then hspeed = min(2, abs(target_x - x)) * sign(target_x - x); // Approach to the target x value
    else hspeed = 0;
    
    if ( target.dead ) then mission_completed = true; // Hurray! We are good. Now he can rest in peace.
} else if not spinning { // Oh gosh. He killed us.
    vspeed = background_vspeed[0];
    hspeed = 0;
}
</string>
