        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Create own sprite if everyhing is set
globalvar distance, cars_killed, last_words;

if sprite == noone  {
    draw_set_halign(fa_left);
    draw_set_valign(fa_top);
    var background_sprite = sprite_index;

    var surf = surface_create(sprite_get_width(background_sprite), sprite_get_height(background_sprite));
    surface_set_target(surf);
    draw_sprite(background_sprite, 0, 0, 0);
    
    var text_x = 120;
    var text_y = 40;
    
    draw_set_color(make_colour_rgb(192,6,123));
    draw_set_font(font_scroll_title);
    var text = "Dear friend,";
    draw_text(text_x, text_y, text);
    text_y += string_height(text);
    
    draw_set_font(font_scroll_text);
    draw_set_color(make_colour_rgb(138,43,226));
    
    var lines;
    lines[0] =  "you have completed many chalanges in this live.";
    lines[1] =  "";
    lines[2] =  "You went " + string(distance) + " m, ";
    lines[3] =  "crashing " + string(cars_killed) + " cars.";
    lines[4] =  "";
    lines[5] =  "We will always remember your last words: ";
    lines[6] =  "";
    
    var max_i = array_length_1d(lines);
    
    for (var i = 0; i &lt; max_i; i++){
        text = lines[i];
        var th = string_height(text);
        draw_text_ext(text_x, text_y, text, th, 350);
        if text == "" then text = "A";
        text_y += string_height_ext(text, th, 350);
    }
    
    draw_set_color(make_colour_rgb(255,0,180) );
    text = '"' + last_words[| irandom(ds_list_size(last_words)-1)] + '"';
    draw_text_ext(text_x, text_y, text, string_height(text), 350);

    sprite = sprite_create_from_surface(surf, 0, 0, sprite_get_width(background_sprite), sprite_get_height(background_sprite), true, true, 0, 0);
    surface_reset_target();
    surface_free(surf);
} else if sprite != noone {
    draw_sprite_ext(sprite, 0, x, y, 1, 1, 0, c_white, alpha);
}
</string>
