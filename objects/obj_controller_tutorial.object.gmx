        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Timeline
tick ++;
if stage &lt; 12 then global.petrol ++; // Keep it infinite

if ((stage == 0) and ((tick / room_speed) &gt;= 5)) {
    last_message = show_info_message(spr_tutorial_ach_info, "Welcome", 
    "Hi there! I am Culbert, your personal assistent. You activated me by entering this cross-sector highway.", 10);
    stage = 0.5;
    tick = 0;
} else if ((stage == 0.5) and (info_message_completed(last_message))) {
    stage = 1;
    tick = 0;
    last_message = noone;
} else if ((stage == 1) and ((tick / room_speed) &gt;= 5)) {
    slowmo_enable();
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "Since this is a highway, better make sure you can drive. Use that round thingy on your right like joystick.", 15);
    obj_touchpad.sprite_index = spr_touchpad;
    obj_touchpad.image_xscale = 1;
    effect_create_above(ef_explosion, obj_touchpad.x, obj_touchpad.y, 2, c_green);
    stage = 2;
    tick = 0;
} else if ((stage == 2) and (info_message_completed(last_message))) {
    slowmo_disable();
    _stage3_last_x = obj_player.x;
    _stage3_last_y = obj_player.y;
    stage = 3;
    tick = 0;
} else if ((stage == 4) and ((tick / room_speed) &gt;= 6)) {
    slowmo_enable();
    last_message = show_info_message(spr_tutorial_ach_info, "Excelent", 
    "Now, when you are able to move, try not to crush into any of other cars on the streets.", 15);
    effect_create_above(ef_explosion, obj_touchpad.x, obj_touchpad.y, 2, c_green);
    obj_touchpad.sprite_index = spr_steering_wheel;
    stage = 5;
    tick = 0;
} else if ((stage == 5) and (info_message_completed(last_message))) {
    slowmo_disable();
    stage = 6;
    tick = 0;
} else if ((stage == 7) and (instance_number(obj_car) == 1)) {     //Wait until all cars from stage 6 are gone ( except obj_player )
    slowmo_enable();
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "In real life, you have limited number of lives, so when you crash, you lose one. Fortunately, there is a solution for that.", 15);
    tick = 0;
    stage = 7.25;
} else if ((stage == 7.25) and (info_message_completed(last_message))) {
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "Crush the ambulance to gain one extra live!", 15);
    stage = 7.3;
    tick = 0;
} else if ((stage == 7.3) and (info_message_completed(last_message))) {
    slowmo_disable();
    instance_create(-1000, 0, obj_ambulance);
    stage = 7.4;
    tick = 0;
}  else if ((stage == 7.5) and ((tick / room_speed) &gt;= 2)) {
    slowmo_enable();
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "Oh no, you alerted the cops! Dont worry though, I will learn you how to handle this.", 15);
    effect_create_above(ef_explosion, obj_draw_wanted_level.x, obj_draw_wanted_level.y, 2, c_green);
    _stage8_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "Quicky Use that button on your left to activate shields and then push those pesky cops out of the map", 15);
    global.wanted_level = 1;
    tick = 0;
    stage = 8;
} else if ((stage == 8) and (info_message_completed(last_message))) {
    var btn_repel = instance_find(obj_btn_repel, 0);
    btn_repel.image_xscale = 1;
    btn_repel.button_enabled = false;
    stage = 9;
    tick = 0;
} else if ((stage == 9) and (info_message_completed(_stage8_message))) {
    slowmo_disable();
    var btn_repel = instance_find(obj_btn_repel, 0);
    btn_repel.button_enabled = true;
    tick = 0;
    last_message = noone;
    stage = 10;
} else if ((stage == 10) and (obj_btn_repel.alarm[0] &gt; 0)) { // User activated shields
    if not _stage10_shield_activated { // Send cop for the first time
        var btn_repel = instance_find(obj_btn_repel, 0);
        btn_repel.button_enabled = false;
        obj_btn_repel.alarm[1] = -1;
        _stage10_shield_activated = true;
        tick = 0;
    } else if (_stage10_shield_activated and not _stage10_police_sent and tick &gt; (2 * room_speed)){
        _stage10_police_sent = true;
        var inst_x = obj_player.x; // Direct approach
        var inst = instance_create(inst_x, (-1 * random(10000)) - 100, obj_police);
        inst.command = choose("LEFT", "RIGHT");
    } else if (_stage10_shield_activated and _stage10_police_sent and instance_number(obj_police) == 0) { // Police has been destroyed. Good job.
        tick = 0;
        last_message = noone;
        stage = 10.1;
    }
    obj_btn_repel.alarm[0] = room_speed;  // Shield shall not fall
} else if ((stage == 10.1) and (tick &gt; (2 * room_speed))) { 
    slowmo_enable();
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "Nice. The second button launches an oil pot that will make all cars spin out of the map on contact.", 15);
    global.wanted_level = 0;
    obj_btn_oil.image_xscale = 1;
    obj_btn_oil.button_enabled = true;
    stage = 10.2;
    tick = 0;
} else if ((stage == 10.2) and (info_message_completed(last_message))) { 
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "Third one calls a dropship. Dropships will drop you some fuel/battery/whatever. You can also shoot down police choppa with it.", 15);
    global.wanted_level = 0;
    obj_btn_call_fuel.image_xscale = 1;
    obj_btn_call_fuel.button_enabled = true;
    stage = 10.3;
    tick = 0;
}  else if ((stage == 10.3) and (info_message_completed(last_message))) { 
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "Hourglass allows you to skip some ability reloading time by decreasing it to one tenth!", 15);
    global.wanted_level = 0;
    obj_btn_cooldown.image_xscale = 1;
    obj_btn_cooldown.button_enabled = true;
    stage = 10.4;
    tick = 0;
} else if ((stage == 10.4) and (info_message_completed(last_message))) { 
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "No we are comming to my favourite one! The special ability button, every car has its own - look into garage to see what it does.", 15);
    global.wanted_level = 0;
    obj_btn_special.image_xscale = 1;
    obj_btn_special.button_enabled = true;
    stage = 10.5;
    tick = 0;
} else if ((stage == 10.5) and (info_message_completed(last_message))) { 
    slowmo_disable();
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "Now I will give you few seconds to try it all out.", 7);
    stage = 11;
    tick = 0;
} else if ((stage == 11) and (tick &gt; (15 * room_speed))) { 
    slowmo_enable();
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "Good. Very good. But there is no more time! We are running out of fuel.", 15);
    global.wanted_level = 0;
    stage = 12;
    tick = 0;
} else if ((stage == 12) and (info_message_completed(last_message))) {
    slowmo_disable();
    tick = 0;
    stage = 13;
} else if ((stage == 13) and (tick &gt; (room_speed * 5))){
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "Grab those thingies on the ground to fill up the fuel.", 10);
    stage = 14;
    tick = 0;
} else if ((stage == 14) and (info_message_completed(last_message)) and (tick &gt; (room_speed * 5)) and (global.petrol &gt;= (0.9 * global.petrol_max))) {
    show_achievement("Learned", 1);
    stage = 15;
    tick = 0;
} else if ((stage == 15) and (tick &gt; (5 * room_speed))) {
    globalvar achievements_vars;
    last_message = show_info_message(spr_tutorial_ach_info, "Culbert", 
    "This was all the basics you need to be able to drive on the streets. Drive safe!", 15);
    stage = 16;
    achievements_vars[? 'tutorial_completed'] = true;
    settings_save();
    obj_player.game_won = true;
    audio_stop_sound(sound_music_cool_castle_in_the_clouds);
    audio_play_sound(sound_music_dancing_on_clouds_looping, 10, true);
} else if ((stage == 16) and (info_message_completed(last_message)) and (tick &gt; (10 * room_speed))) {
    room_goto(room_main_menu_future);
}
</string>
