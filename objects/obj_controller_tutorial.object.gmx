            <string>///Stage 3
if stage == 3 {
    if not _stage3_move_x then _stage3_move_x = abs(_stage3_last_x - obj_player.x) &gt; obj_player.sprite_width;
    if not _stage3_move_y then _stage3_move_y = abs(_stage3_last_y - obj_player.y) &gt; obj_player.sprite_height;
    
    if _stage3_move_x and _stage3_move_y {
        show_achievement("First turns", 1);
        stage = 4;
