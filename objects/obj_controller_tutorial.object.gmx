            <string>///Stage 6
if stage == 6 {
    if tick % (room_speed * 3) == 0 { // Send car every 3 seconds
        var inst = instance_create((-1 * random(10000)) + 100, (-1 * random(10000)) + 100, obj_car);
        if instance_exists(inst) then inst.vspeed /= 4; // Make it easier for player to evade
    }
    
